{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "almond",
  "name": "PUG (Pure Upgrade)",
  "colors": {
    "primary": "#8648e5",
    "light": "#a44af2",
    "dark": "#5e4ddb"
  },
  "favicon": {
    "light": "/logo/pug-light-favicon.png",
    "dark": "/logo/pug-dark-favicon.png"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "quickstart",
              "overview",
              "cli-install"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Blog",
          "href": "https://getpug.dev",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/pug-light-favicon.png",
    "dark": "/logo/pug-light-favicon.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:founders@getpug.dev"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://www.getpug.dev/login"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/getpugdev",
      "linkedin": "https://linkedin.com/company/getpugdev"
    }
  }
